{
    "presets": [
      [
        "@babel/preset-env",
        {
            "useBuiltIns": "usage", // "usage" | "entry" | false, defaults to false.
            "corejs": "3",
            "targets": {
                "esmodules": true,
                "ie": "11"
            }
        }
      ]
    ],
    "plugins": [ 
        "@babel/plugin-proposal-dynamic-import",
        "@babel/plugin-transform-modules-amd"
    ]
}